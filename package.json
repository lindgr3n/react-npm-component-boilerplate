{
  "name": "react-npm-component-boilerplate",
  "version": "1.0.0",
  "description": "Simple react npm package boilerplate.",
  "repository": {
    "type": "git",
    "url": "https://github.com/lindgr3n/react-npm-component-boilerplate.git"
  },
  "author": "Johan Lindgren",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lindgr3n/react-npm-component-boilerplate.git/issues"
  },
  "homepage": "https://github.com/lindgr3n/react-npm-component-boilerplate",
  "keywords": [
    "react",
    "react-component",
    "boilerplate",
    "npm",
    "module",
    "template",
    "webpack 2"
  ],
  "scripts": {
    "build": "webpack --config webpack.config.babel.js",
    "release": "webpack --p --config webpack.config.babel.js",
    "test": "jest",
    "coverage": "jest --coverage",
    "deploy": "",
    "lint": "eslint ./src"
  },
  "peerDependencies": {
    "react": "~0.14.8 || ^15.0.0",
    "react-dom": "~0.14.8 || ^15.0.0"
  },
  "dependencies": {
    "react": "^15.5.4",
    "react-cookie": "^1.0.5"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.2",
    "babel-jest": "^19.0.0",
    "babel-loader": "^7.0.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "chai": "^3.5.0",
    "clean-webpack-plugin": "^0.1.16",
    "css-loader": "^0.28.0",
    "enzyme": "^2.8.2",
    "eslint": "^3.19.0",
    "eslint-plugin-react": "^6.10.3",
    "jest": "^19.0.2",
    "react-addons-test-utils": "^15.5.1",
    "react-dom": "^15.5.4",
    "react-test-renderer": "^15.5.4",
    "style-loader": "^0.16.1",
    "webpack": "^2.4.1"
  },
  "jest": {
    "modulePaths": [
      "./src"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ]
  }
}
